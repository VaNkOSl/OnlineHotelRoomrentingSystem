@model IEnumerable<HotelServiceModel>
@{
    ViewBag.Title = "Mine Hotels";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (!Model.Any())
{
    <h2 class="text-center">You have no hotels yet!</h2>
}

<div class="row">
    @foreach (var hotel in Model)
    {
        <partial name="_HotelPartial" model="@hotel" />
    }
</div>