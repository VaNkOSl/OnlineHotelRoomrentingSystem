@model IEnumerable<RoomServiceModel>
@{
    ViewBag.Title = "Mine Rooms";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (Model == null || !Model.Any())
{
    <h2 class="text-center">You have no rooms yet!</h2>
}
else
{
    <div class="row">
        @foreach (var room in Model)
        {
            <partial name="_RoomPartial" model="@room" />
        }
    </div>
}